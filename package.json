{
  "name": "astro",
  "type": "module",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "start": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^5.16.6",
    "solid-js": "^1.9.10",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/solid-js": "^5.1.3",
    "@tailwindcss/vite": "^4.1.18"
  },
  "devDependencies": {
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
