{
  "name": "astro",
  "type": "module",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "start": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/solid-js": "^4.4.2",
    "@astrojs/tailwind": "^5.1.1",
    "astro": "^4.15.11",
    "solid-js": "^1.9.1"
  },
  "devDependencies": {
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
